<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <!--Progress Bar (Secondary, Outer Ring)-->
        <shape
            android:shape="ring"
            android:thicknessRatio="12"
            android:useLevel="false">
            <solid android:color="@color/progressBarBackgroundColor"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="12"
            android:useLevel="true">
            <solid android:color="@color/progressBarSecondaryColor"/>
        </shape>
    </item>
    <item>
        <shape
            android:shape="ring"
            android:thicknessRatio="24"
            android:useLevel="false">
            <solid android:color="@color/progressBarBackgroundColor"/>
        </shape>
    </item>

</layer-list>